<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Gasolin</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Gasolin.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
       <div class="row header-navbar">
                <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light box-shadow mb-3">
           
                    <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                        <div class="row">
                            <ul class="navbar-nav flex-grow-1">

                                    <div class="col-4">
                                    </div>
                                    <div class="col-lg-10">
                                        <li class="nav-item">
                                            <button1>
                                                <a1 asp-page="/Index"><span>Tidslinje</span></a1>
                                            </button1>
                                        </li>
                                    </div>


       
                                    <div class="col-lg-10"> <li class="nav-item">
                                        <button1>
                                        <a1 asp-page="/Index"><span>Medlemmer</span></a1>
                                        </button1>
                                    </li></div>

                                    <div class="col-lg-4">
                                        <li class="nav-item">
                                            <button1>
                                                <a1 asp-page="/Index"><span>Forside</span></a1>
                                            </button1>
                                        </li>
                                    </div>
                            </ul>
                        </div>
                    </div>
 
                </nav>
        </div>

     

    </header>
    <div class="container" style="margin-top: 10%">
 
        <main role="main" class="pb-3, MarginTopCustom">
            
            @RenderBody()

        </main>

    </div>

    <footer class="border-top site-footer text-muted">
        <div class="container">
            &copy; 2025 - Gasolin - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>