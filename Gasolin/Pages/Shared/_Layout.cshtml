<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Gasolin</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Gasolin.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav>
            <ul class="topnav">
                <li></li>
                <li><a href="/Index">Forside</a></li>
                <li><a href="/Index#Medlemmer">Medlemmer</a></li>
                <li><a href="/Index#timeline">Tidslinje</a></li>
                <li><a href="/memorabilia">Memorabilia</a></li>
                <li></li>
            </ul>
        </nav>
    </header>
    <div class="container" style="margin-top:50px">
 
        <main role="main" class="pb-3, MarginTopCustom">
            
            @RenderBody()

        </main>

    </div>

    <footer class="border-top site-footer text-muted">
        <div class="container">
            &copy; 2025 - Gasolin - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>