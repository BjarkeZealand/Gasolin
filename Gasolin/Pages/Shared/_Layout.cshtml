<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Gasolin</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Gasolin.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow mb-3">
            <div class="minMargin">
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <button1>
                               <a1 asp-page="/Index"><span>Tidslinje</span></a1>
                            </button1>
                                </div>
                        </li>
                        <li class="nav-item">
                            <button2>
                                <a1 asp-page="/Index"><span>Medlemmer</span></a1>
                            </button2>
                        </li>
                        <li class="nav-item">
                            <button3>
                                <a1 asp-page="/Index"><span>Forside</span></a1>
                            </button3>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link custom nav link" asp-area="" asp-page="/Privacy">Forside</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>


        <div class="row">
            <div class="col-sm-4 bg-primary text-white"></div>
            <div class="col-sm-4 bg-dark text-white">33.33%</div>
            <div class="col-sm-4 bg-primary text-white">33.33%</div>
        </div>
       
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top site-footer text-muted">
        <div class="container">
            &copy; 2025 - Gasolin - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>