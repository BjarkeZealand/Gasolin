@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<body class="bg-black">
    <main class="bg-black" >
            <h1 class="centered-text bg-black">Masser Af Succes</h1>
		<img class="pictures" img src= "~/pictures/gasolin final pic.jpg" />

            <div class="container-fluid">
                <div class="card mx-auto bg-dark text-light" style="width: 26rem;">
                    <img class="card-img-top" src="@Model.members[0].ImgPath" alt="Card image cap">
                    <div class="card-body">
                        <a href="@Model.members[0].BioLink" class="btn btn-warning">@Model.members[0].Name</a>
                        <p class="text-light mt-3"><i>@Model.members[0].Role</i></p>
                        <p class="card-text">@Model.members[0].YearOfBirth - @Model.members[0].YearOfDeath</p>
                    </div>
                </div>
            <div class="row justify-content-center">
                @for (int i = 1; i < Model.members.Length; i++)
                {
                    <div class="card mx-sm-auto bg-dark text-light" style="display: inline-block; width: 18rem;">
                        <img class="card-img-top" src="@Model.members[i].ImgPath" alt="Card image cap">
                        <div class="card-body">
                            <a href="@Model.members[i].BioLink" class="btn btn-warning">@Model.members[i].Name</a>
                            <p class="text-light mt-3"><i>@Model.members[i].Role</i></p>
                            <p class="card-text">@Model.members[i].YearOfBirth - @Model.members[i].YearOfDeath</p>
                        </div>
                    </div>
                }
            </div>
            </div>

    </main>
